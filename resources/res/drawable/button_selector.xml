<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#003333" android:endColor="#003333" android:angle="180"/>
            <stroke android:width="2dp" android:color="#336666"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#339999" android:endColor="#339999" android:angle="180"/>
            <stroke android:width="2dp" android:color="#99cc99"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#66cccc" android:endColor="#006666" android:angle="270"/>
            <stroke android:width="1dp" android:color="#2e2e2e"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
</selector>
